{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-object-rest-spread",
      [
        "babel-plugin-module-resolver",
        {
          "root": [
            "./server"
          ],
          "alias": {
            "@controllers": "./server/controllers"
          }
        }
      ]
    ]
  }