{
  "name": "gtb-backend",
  "version": "1.0.0",
  "description": "Backend",
  "main": "app.js",
  "scripts": {
    "start": "node ./dist/app.js",
    "start:dev": "DEBUG=app nodemon ./server/app.js --exec babel-node",
    "build": "npm run clean && babel ./server -d ./dist",
    "clean": "rm -rf dist",
    "create:db": "NODE_ENV=development babel-node ./server/db/runScript.js",
    "create:distributors": "NODE_ENV=development babel-node ./server/db/createDistributors.js",
    "create:manufacturer": "NODE_ENV=development babel-node ./server/db/createManufacturer.js",
    "create:bank": "NODE_ENV=development babel-node ./server/db/createBank.js",
    "create:product": "NODE_ENV=development babel-node ./server/db/createProduct.js",
    "create:account": "NODE_ENV=development babel-node ./server/db/createAccount.js",
    "create:paymentoption": "NODE_ENV=development babel-node ./server/db/createPaymentOption.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "rm -rf ./server/database_test.sqlite3 && NODE_ENV=test npm run refreshdb",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text --require @babel/register --env=test mocha --timeout 10000 ./server/tests/**/*.test.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmygozi/gtb-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emmygozi/gtb-backend/issues"
  },
  "homepage": "https://github.com/emmygozi/gtb-backend#readme",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@sendgrid/mail": "^6.4.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "history": "^4.7.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mailgen": "^2.0.7",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "sinon": "^7.3.2",
    "slugify": "^1.3.4",
    "sqlite3": "^4.0.8",
    "twilio": "^3.38.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.4",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
